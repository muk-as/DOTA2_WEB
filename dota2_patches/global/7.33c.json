  "generic": [
    {
      "indent_level": 1,
      "note": "Picking up a wisdom rune now also grants experience to the lowest-level hero on the team (if the lowest-level hero picks up the rune, experience goes to the second-lowest-level hero)"
    },
    {
      "indent_level": 1,
      "note": "Observer Ward experience bounty increased from 50+6/min to 70+8/min"
    },
    {
      "indent_level": 1,
      "note": "When a stacked camp is killed, the player that did the stacking receives 30% of experience gained (same percentage as gold bounty)"
    },
    {
      "indent_level": 1,
      "note": "Tormentor Reflect now always affects attacking hero, regardless of distance"
    },
    {
      "indent_level": 1,
      "note": "Cleared multiple tree paths that led to dead ends and deceptive looking terrain"
    },
    {
      "indent_level": 1,
      "note": "Heroes bonus attribute points can now be skilled starting at level 6"
    },
    {
      "indent_level": 1,
      "note": "Universal Heroes damage per attribute increased from 0.6 to 0.7"
    }
  ],
