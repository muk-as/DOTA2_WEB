  "ti11_immortal_1_9_desc": "Bundle avec des effets personnalisés pour Sharpshooter et une raillerie",
  "ti11_immortal_1_10_name": "Emblème du passe de bataille de 2022",
  "ti11_immortal_1_10_desc": "Item dont on peut équiper l'ensemble des héros et héroïnes, avec des effets personnalisés exclusifs",
  "ti11_immortal_1_11_name": "Emblème du passe de bataille de 2022",
  "ti11_immortal_1_11_desc": "Item dont on peut équiper l'ensemble des héros et héroïnes, avec des effets personnalisés exclusifs",
  "ti11_immortals_1_levels_title": "Recevez un trésor I aux niveaux",
  "ti11_immortals_1_levels": "1 | 21 | 47 | 76 | 109 | 229 | 251",
  "ti11_immortals_1_levels_desc": "puis tous les 30 niveaux après le niveau 251",
  "ti11_immortal_2_1_name": "Pattes de Lycosidae",
  "ti11_immortal_2_1_desc": "Pattes avec des effets personnalisés pour Spin Web",
  "ti11_immortal_2_2_name": "Crinière du dragon",
  "ti11_immortal_2_2_desc": "Heaume avec des effets personnalisés pour Life Break",
